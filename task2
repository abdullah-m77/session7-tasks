#include <iostream>

using namespace std;

int main()
{
     int n;
    cout<<"enter number: ";
    cin>>n;
    int *arr=new int[n];
    for(int i=0; i<n; i++){
        arr[i]=2*(i+1);
    }
    cout << n<< endl;
    for(int i=0; i<n; i++){
        cout<<arr[i]<<" ";
    }
    delete[]arr;
    return 0;
}
